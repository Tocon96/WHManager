<Application x:Class="WHManager.DesktopUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WHManager.DesktopUI"
             xmlns:viewmodels="clr-namespace:WHManager.BusinessLogic.ViewModels;assembly=WHManager.BusinessLogic"
             xmlns:warehouseviews="clr-namespace:WHManager.DesktopUI.Views.WarehouseViews"
             StartupUri="AddProductButton.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type viewmodels:ProductViewModel}">
                <warehouseviews:ProductView/>
            </DataTemplate>
        </ResourceDictionary>
    </Application.Resources>
</Application>